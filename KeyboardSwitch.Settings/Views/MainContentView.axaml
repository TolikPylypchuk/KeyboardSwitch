<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:KeyboardSwitch.Settings.Views"
             xmlns:l="using:KeyboardSwitch.Settings.Localization"
             x:Class="KeyboardSwitch.Settings.Views.MainContentView">
    <TabControl>
        <TabItem x:Name="CharMappingTabItem" Header="{l:Translate CharMapping}" />
        <TabItem x:Name="PreferencesTabItem" Header="{l:Translate Preferences}" />
        <TabItem x:Name="ConverterTabItem" Header="{l:Translate Converter}" />
        <TabItem x:Name="ConverterSettingsTabItem" Header="{l:Translate ConverterSettings}" />
        <TabItem x:Name="SandboxTabItem" Header="{l:Translate Sandbox}">
            <local:SandboxView />
        </TabItem>
        <TabItem x:Name="AboutTabItem" Header="{l:Translate About}" />
    </TabControl>
</UserControl>
