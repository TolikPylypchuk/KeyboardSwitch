<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:l="using:KeyboardSwitch.Settings.Localization"
             x:Class="KeyboardSwitch.Settings.Views.LoadableLayoutView">
    <StackPanel Orientation="Horizontal">
        <TextBlock x:Name="NameTextBlock" Margin="10" FontSize="{DynamicResource FontSizeLarge}"
                   VerticalAlignment="Center" />

        <Button x:Name="DeleteButton" Content="{l:Translate Delete}" Margin="10" VerticalAlignment="Center" />
    </StackPanel>
</UserControl>
