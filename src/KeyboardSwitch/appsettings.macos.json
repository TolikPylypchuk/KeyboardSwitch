{
  "Settings": {
    "SettingsFilePath": "%HOME%/Library/Application Support/Keyboard Switch/settings.json",
    "StateFilePath": "%HOME%/Library/Application Support/Keyboard Switch/state.json",
    "ServicePath": "io.tolik.keyboardswitch",
    "InitialSetupFilePath": "%HOME%/Library/Application Support/Keyboard Switch/.setup-configured"
  },
  "Launchd": {
    "ServiceName": "io.tolik.keyboardswitch",
    "ServiceDescriptorPath": "/Library/LaunchAgents/io.tolik.keyboardswitch.plist"
  },
  "Logger": {
    "LogFilePath": "%HOME%/Library/Application Support/Keyboard Switch/KeyboardSwitch.log",
    "MinimumLevel": "Information",
    "MaxFileSize": 10000000,
    "MaxRetainedFiles": 2,
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
  }
}
